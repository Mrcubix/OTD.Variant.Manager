<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:OTD.Variant.Manager.UX.ViewModels"
             xmlns:menus="clr-namespace:OTD.Variant.Manager.UX.Controls.Menus"
             mc:Ignorable="d" d:DesignWidth="720" d:DesignHeight="600"
             x:Class="OTD.Variant.Manager.UX.Views.MainView"
             x:DataType="vm:MainViewModel">
  <Design.DataContext>
    <vm:MainViewModel />
  </Design.DataContext>

  <Grid>
    <ContentControl Content="{Binding NextViewModel}" />
  </Grid>

</UserControl>
